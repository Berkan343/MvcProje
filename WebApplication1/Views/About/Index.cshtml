@using EntityLayer.Concrete
@model List<About>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h2>hakkımızda veri listesi</h2>
<table class="table table-bordered">
    <tr>
        <th>ID</th>
        <th>Hakkımda başlığı</th>
        <th>Aktif yap</th>
        <th>Pasif yap</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <th>@item.AboutID</th>
            <th>başlık</th>
            <th><a href="#" class="btn btn-warning">aktif ayap</a></th>
            <th><a href="#" class="btn btn-danger">pasif ayap</a></th>
        </tr>
    }
</table>


@Html.Action("AboutPartial","About")

